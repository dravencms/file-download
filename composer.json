{
  "name": "dravencms/file-download",
  "type": "dravencms-package",
  "description": "File download module for dravencms",
  "keywords": [
    "dravencms",
    "file-download"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/file": "^v2.0",
    "dravencms/paginator": ">=1.0"
  },
  "suggest": {
    "dravencms/structure": "Allows file download components to be used in cms"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": "dravencms.config.neon"
    }
  }
}
